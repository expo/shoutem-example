{
  "expo": {
    "name": "@shoutem/ui examples",
    "description": "Shoutem UI toolkit in Exponent",
    "slug": "shoutem-ui-examples",
    "sdkVersion": "28.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "privacy": "unlisted",
    "icon": "./assets/images/icon.png",
    "splash": {
      "backgroundColor": "#cccccc",
      "image": "./assets/loading/splash.png",
      "resizeMode": "cover"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
